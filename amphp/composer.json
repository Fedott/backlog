{
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Fedot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fedot\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "amphp/aerys": "dev-amp_v2",
        "amphp/redis": "dev-amp_v2",
        "amphp/amp": "^2@dev",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.6",
        "ramsey/uuid": "^3.4",
        "symfony/property-access": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/property-info": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/config": "^3.3",
        "symfony/console": "^3.3",
        "symfony/finder": "^3.3",
        "fedot/async-data-mapper": "0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Fedott/redis.git"
        }
    ]
}
