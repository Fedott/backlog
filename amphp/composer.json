{
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Fedot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fedot\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "amphp/aerys": "dev-amp_v2",
        "amphp/redis": "dev-amp_v2",
        "amphp/loop": "dev-master",
        "symfony/serializer": "^3.2",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.6",
        "symfony/property-access": "^3.2",
        "ramsey/uuid": "^3.4",
        "symfony/property-info": "^3.2",
        "symfony/dependency-injection": "3.3.x-dev",
        "symfony/yaml": "^3.2",
        "symfony/config": "^3.2",
        "symfony/console": "^3.2",
        "fedot/async-data-mapper": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    }
}
